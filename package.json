    "getmac": "^1.2.1"
  },
  "devDependencies": {
    "coffeelint-stylish": "^0.1.2",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-coffeelint": "^0.6.0",
    "gulp-debug": "^3.1.0",
    "gulp-jsonlint": "^1.2.0",
    "gulp-lesshint": "^4.0.0",
