{
  "name": "local-storage",
  "main": "./lib/local-storage",
  "version": "0.7.3",
  "description": "Edit items stored in Atom's localStorage",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode none --watch",
    "lint": "gulp lint",
    "test": "npm run lint"
  },
  "keywords": [
    "localStorage",
    "local storage"
  ],
  "activationCommands": {
    "atom-workspace": [
      "local-storage:open-item",
      "local-storage:delete-item",
      "local-storage:save-item"
    ]
  },
  "repository": "https://github.com/idleberg/atom-local-storage",
  "license": "MIT",
  "engines": {
    "atom": ">=1.17.0 <2.0.0"
  },
  "dependencies": {
    "atom-select-list": "^0.7.2"
  },
  "devDependencies": {
    "coffee-loader": "^0.9.0",
    "coffeelint-stylish": "^0.1.2",
    "coffeescript": "^2.4.1",
    "gulp": "^4.0.2",
    "gulp-coffeelint": "^0.6.0",
    "gulp-debug": "^4.0.0",
    "gulp-jsonlint": "^1.3.0",
    "husky": "^3.0.2",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5"
  }
}
