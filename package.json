{
  "name": "local-storage",
  "main": "./lib/local-storage",
  "version": "0.7.0",
  "description": "Edit items stored in Atom's localStorage",
  "scripts": {
    "precommit": "npm run test",
    "test": "gulp lint"
  },
  "keywords": [
    "localStorage",
    "local storage"
  ],
  "activationCommands": {
    "atom-workspace": [
      "local-storage:open-item",
      "local-storage:delete-item",
      "local-storage:save-item"
    ]
  },
  "repository": "https://github.com/idleberg/atom-local-storage",
  "license": "MIT",
  "engines": {
    "atom": ">=1.17.0 <2.0.0"
  },
  "dependencies": {
    "atom-select-list": "^0.7.1",
    "getmac": "^1.4.1"
  },
  "devDependencies": {
    "coffeelint-stylish": "^0.1.2",
    "gulp": "^4.0.0",
    "gulp-coffeelint": "^0.6.0",
    "gulp-debug": "^4.0.0",
    "gulp-jsonlint": "^1.2.1",
    "husky": "^0.14.3"
  }
}
